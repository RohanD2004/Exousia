{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["google"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [],
    "rules": {
        "object-curly-spacing": "on",
        "indent": "on",
        "require-jsdoc": "on",
        "max-len": "on",
        "operator-linebreak": "on",
        "no-invalid-this": "on",
        "new-cap": "on",
        "space-before-function-paren": "on"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
